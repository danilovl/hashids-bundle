{
    "name": "danilovl/hashids-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle provides integrates hashids",
    "keywords": ["php","symfony","bundle","twig extension"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Danilov"
        }
    ],
    "autoload": {
        "psr-4": {
            "Danilovl\\HashidsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Danilovl\\HashidsBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "ext-gmp": "*",
        "php": "^8.3",
        "doctrine/orm": "^2.9",
        "hashids/hashids": "^5.0",
        "symfony/doctrine-bridge": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    }
}
